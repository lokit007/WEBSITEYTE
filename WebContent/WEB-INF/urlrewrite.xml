<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">

<!--
    Configuration file for UrlRewriteFilter
    http://tuckey.org/urlrewrite/
-->
<urlrewrite>
    <rule>
        <note>Admin Trang Chu</note>
        <from>/admin/trang-chu.html</from>
        <to>/admin/trang-chu.action</to>
    </rule>
    <rule>
        <note>Admin Dang Nhap</note>
        <from>/admin/dang-nhap.html</from>
        <to>/admin/dang-nhap.action</to>
    </rule>
    <rule>
        <note>Esempio 1.2</note>
        <from>/getdocument_2\.jsp\?id=(\d+)</from>
        <to>/documents/index.jsp?docid=$1</to>
    </rule>
    
    <rule>
      <note>Esempio 1.3</note>
      <from>/docu/(\d+).html</from>
      <to>/documents/index.jsp?id=$1</to>
    </rule>

</urlrewrite>
